digraph {
	rankdir=LR size=12
	Input [label="Input Features" fillcolor=lightblue shape=ellipse style=filled]
	subgraph cluster_base1 {
		node [fillcolor=orange style=filled]
		color=orange label="BaseNN1 (Shallow)"
		B1_fc1 [label="FC1
ReLU"]
		B1_fc2 [label="FC2
ReLU"]
		B1_dropout [label="Dropout(0.2)"]
		B1_out [label="Output Logit"]
		B1_fc1 -> B1_fc2
		B1_fc2 -> B1_dropout
		B1_dropout -> B1_out
	}
	subgraph cluster_base2 {
		node [fillcolor=green style=filled]
		color=green label="BaseNN2 (Deep)"
		B2_fc1 [label="FC1
Tanh"]
		B2_fc2 [label="FC2
Tanh"]
		B2_fc3 [label="FC3
Tanh"]
		B2_dropout [label="Dropout(0.1)"]
		B2_out [label="Output Logit"]
		B2_fc1 -> B2_fc2
		B2_fc2 -> B2_fc3
		B2_fc3 -> B2_dropout
		B2_dropout -> B2_out
	}
	Voting [label="Fuzzy Voting Layer
(weighted avg)" fillcolor=plum shape=box style=filled]
	Final [label="Final Linear Layer
Output Logit" fillcolor=lightgrey shape=box style=filled]
	Input -> B1_fc1
	Input -> B2_fc1
	B1_out -> Voting [label=sigmoid]
	B2_out -> Voting [label=sigmoid]
	Voting -> Final
}
